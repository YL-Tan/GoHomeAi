# Ignore environment files
.env
/backend/.env
/frontend/.env

# Ignore database files
/backend/*.accdb
/backend/*.db
/backend/*.dbf
/backend/*.mdb
/backend/*.pdb
/backend/*.sqlite3
/backend/*.db-shm
/backend/*.db-wal

# Ignore Go-related files
/backend/*.exe
/backend/*.exe~
/backend/*.dll
/backend/*.so
/backend/*.dylib
/backend/*.test
/backend/*.out
/backend/go.work

# Ignore Python virtual environments
/backend/.venv
/backend/env/
/backend/venv/
/backend/ENV/

# Ignore Python cache & package manager files
/backend/__pycache__/
/backend/*.py[cod]
/backend/.pytest_cache/
/backend/.mypy_cache/
/backend/.ruff_cache/

# Ignore frontend dependencies
/frontend/node_modules/
/frontend/.next/
/frontend/dist/
/frontend/build/

# Ignore logs and temporary files
logs/
*.log
/tmp/
